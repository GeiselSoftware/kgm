PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX gse: <gse:>
PREFIX : <http://example.com/#>

:alice rdf:type :Human; rdf:type foaf:Person .
:bob rdf:type :Human; rdf:type foaf:Person .
:alice foaf:name "Alice" .
:bob foaf:name "Bob" .
:alice foaf:friendOf :bob .
:bob foaf:friendOf :alice .
:alice gse:name "Alice"; gse:node_vis_color "green".
:bob gse:name "Bob"; gse:node_vis_color "cyan".

:amy rdf:type :Dog; gse:name "Amy"; gse:node_vis_color "brown" .
:amy :ownedBy :alice .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix dash: <http://datashapes.org/dash#> .

PREFIX gse: <gse:>
PREFIX : <orh:>

:Human rdf:type rdfs:Class; rdf:type sh:NodeShape;
       sh:property [sh:path :name; sh:minCount 1; sh:maxCount 1; sh:dataclass xsd:string];
       sh:property [sh:path :friendOf; sh:minCount 0; sh:class :Human] .

:Dog rdf:type rdfs:Class; rdf:type sh:NodeShape;
       sh:property [sh:path :name; sh:minCount 1; sh:maxCount 1; sh:dataclass xsd:string];
       sh:ownedBy [sh:minCount 1; sh:maxCount 1; sh:class :Human] .

:Human sh:property [sh:path gse:node_vis_color; sh:minCount 0; sh:maxCount 1; sh:dataclass xsd:string] .
:Dog sh:property [sh:path gse:node_vis_color; sh:minCount 0; sh:maxCount 1; sh:dataclass xsd:string] .

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX gse: <gse:>
PREFIX : <simple:>

:alice rdf:type :Human.
:bob rdf:type :Human.
:alice :friendOf :bob .
:bob :friendOf :alice .
:alice :name "Alice"; gse:node_vis_color "green".
:bob :name "Bob"; gse:node_vis_color "cyan".

:amy rdf:type :Dog; :name "Amy"; gse:node_vis_color "brown" .
:amy :ownedBy :alice .
